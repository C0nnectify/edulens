{
  "gpa_patterns": [
    {
      "pattern": "(?i)gpa[:\\s]*([0-9]\\.[0-9]{1,2})\\s*/\\s*([0-9]\\.[0-9]{1,2})",
      "description": "GPA format: 3.8/4.0",
      "groups": {
        "gpa": 1,
        "scale": 2
      }
    },
    {
      "pattern": "(?i)gpa[:\\s]*([0-9]\\.[0-9]{1,2})\\s*out\\s*of\\s*([0-9]\\.[0-9]{1,2})",
      "description": "GPA format: 3.8 out of 4.0",
      "groups": {
        "gpa": 1,
        "scale": 2
      }
    },
    {
      "pattern": "(?i)gpa[:\\s]*([0-9]{1,2})\\s*/\\s*([0-9]{1,2})",
      "description": "GPA format: 9/10",
      "groups": {
        "gpa": 1,
        "scale": 2
      }
    },
    {
      "pattern": "(?i)cgpa[:\\s]*([0-9]\\.[0-9]{1,2})",
      "description": "CGPA without scale (assume 10.0)",
      "groups": {
        "gpa": 1
      },
      "default_scale": 10.0
    },
    {
      "pattern": "(?i)percentage[:\\s]*([0-9]{1,3})%",
      "description": "Percentage format: 85%",
      "groups": {
        "percentage": 1
      }
    }
  ],
  "gre_patterns": [
    {
      "pattern": "(?i)gre[:\\s]*v[erbal]*[:\\s]*([0-9]{3})[,\\s]*q[uant]*[:\\s]*([0-9]{3})[,\\s]*a[w]*[:\\s]*([0-9]\\.[0-9])",
      "description": "GRE format: V:165, Q:170, AW:5.0",
      "groups": {
        "verbal": 1,
        "quant": 2,
        "aw": 3
      }
    },
    {
      "pattern": "(?i)gre[:\\s]*([0-9]{3})v[,\\s]*([0-9]{3})q[,\\s]*([0-9]\\.[0-9])a",
      "description": "GRE format: 165V, 170Q, 5.0A",
      "groups": {
        "verbal": 1,
        "quant": 2,
        "aw": 3
      }
    },
    {
      "pattern": "(?i)verbal[:\\s]*([0-9]{3})",
      "description": "GRE Verbal only",
      "groups": {
        "verbal": 1
      }
    },
    {
      "pattern": "(?i)quant[itative]*[:\\s]*([0-9]{3})",
      "description": "GRE Quant only",
      "groups": {
        "quant": 1
      }
    },
    {
      "pattern": "(?i)(?:analytical writing|aw|writing)[:\\s]*([0-9]\\.[0-9])",
      "description": "GRE AW only",
      "groups": {
        "aw": 1
      }
    }
  ],
  "toefl_patterns": [
    {
      "pattern": "(?i)toefl[:\\s]*([0-9]{2,3})",
      "description": "TOEFL score: 110",
      "groups": {
        "toefl": 1
      }
    },
    {
      "pattern": "(?i)toefl[:\\s]*(ibt)?[:\\s]*([0-9]{2,3})",
      "description": "TOEFL iBT: 110",
      "groups": {
        "toefl": 2
      }
    }
  ],
  "ielts_patterns": [
    {
      "pattern": "(?i)ielts[:\\s]*([0-9]\\.[0-9])",
      "description": "IELTS score: 8.5",
      "groups": {
        "ielts": 1
      }
    }
  ],
  "research_patterns": [
    {
      "pattern": "(?i)(\\d+)\\s*(?:research\\s*)?(?:paper|publication|pub)s?",
      "description": "Research publications count",
      "groups": {
        "count": 1
      }
    },
    {
      "pattern": "(?i)(\\d+)\\s*years?\\s*(?:of\\s*)?research",
      "description": "Years of research experience",
      "groups": {
        "years": 1
      }
    },
    {
      "pattern": "(?i)(?:first|second|third)\\s*author",
      "description": "Authorship mention",
      "value": 1
    },
    {
      "pattern": "(?i)conference\\s*paper",
      "description": "Conference paper mention",
      "value": 1
    },
    {
      "pattern": "(?i)journal\\s*paper",
      "description": "Journal paper mention",
      "value": 1
    }
  ],
  "international_patterns": [
    {
      "pattern": "(?i)international",
      "description": "Explicit international mention",
      "value": true
    },
    {
      "pattern": "(?i)indian",
      "description": "Indian student",
      "value": true
    },
    {
      "pattern": "(?i)chinese",
      "description": "Chinese student",
      "value": true
    },
    {
      "pattern": "(?i)asian",
      "description": "Asian student",
      "value": true
    },
    {
      "pattern": "(?i)undergrad[uate]*\\s*(?:from|at)\\s*[A-Z]",
      "description": "Undergrad institution pattern",
      "extract": true
    }
  ],
  "institution_patterns": [
    {
      "pattern": "(?i)(?:undergrad|undergraduate|ug)\\s*(?:from|at|:)\\s*([A-Z][A-Za-z\\s]+(?:University|Institute|College))",
      "description": "Undergraduate institution",
      "groups": {
        "institution": 1
      }
    },
    {
      "pattern": "(?i)(?:iit|nit|bits)\\s*([a-z]+)",
      "description": "Indian institutes",
      "groups": {
        "institution": 0
      }
    }
  ],
  "funding_patterns": [
    {
      "pattern": "(?i)full\\s*funding",
      "description": "Full funding mention",
      "value": "Full funding"
    },
    {
      "pattern": "(?i)stipend",
      "description": "Stipend mention",
      "value": "Stipend"
    },
    {
      "pattern": "(?i)fellowship",
      "description": "Fellowship mention",
      "value": "Fellowship"
    },
    {
      "pattern": "(?i)assistantship|ta|ra",
      "description": "Assistantship mention",
      "value": "Assistantship"
    },
    {
      "pattern": "(?i)no\\s*funding|unfunded|self[\\s-]*funded",
      "description": "No funding mention",
      "value": "No funding"
    },
    {
      "pattern": "(?i)\\$([0-9,]+)(?:/year|per year|annually)?",
      "description": "Funding amount",
      "groups": {
        "amount": 1
      }
    }
  ],
  "decision_patterns": [
    {
      "pattern": "(?i)accept(?:ed|ance)",
      "decision": "Accepted"
    },
    {
      "pattern": "(?i)admit(?:ted)?",
      "decision": "Accepted"
    },
    {
      "pattern": "(?i)reject(?:ed|ion)",
      "decision": "Rejected"
    },
    {
      "pattern": "(?i)waitlist(?:ed)?",
      "decision": "Waitlisted"
    },
    {
      "pattern": "(?i)pending",
      "decision": "Pending"
    }
  ]
}
